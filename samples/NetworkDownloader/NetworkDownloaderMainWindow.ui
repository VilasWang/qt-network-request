   <string notr="true">/* VSCode-Inspired Professional Dark Theme - Qt5 Compatible */
/* ========================================
   DESIGN SYSTEM CONSTANTS
   ======================================== */

/* Color Definitions (Qt5 compatible) */
/* Background Colors */
/* BG Primary: #1e1e1e, BG Secondary: #252526, BG Tertiary: #2d2d30 */
/* Border Colors: Light #3e3e42, Medium #4e4e52 */
/* Text Colors: Primary #cccccc, Secondary #969696, Disabled #6a6a6a */
/* Accent Colors: Blue #0078d4, Green #107c10, Orange #ff8c00, Red #e81123 */
/* State Colors: Hover #106ebe, Pressed #005a9e */

/* ========================================
   GLOBAL STYLES
   ======================================== */

QMainWindow {
    background-color: #1e1e1e;
    color: #cccccc;
    font-family: 'Segoe UI', 'SF Pro Display', -apple-system, BlinkMacSystemFont, Arial, sans-serif;
    font-size: 13px;
    font-weight: 400;
}

QWidget#centralWidget {
    background-color: #1e1e1e;
    color: #cccccc;
}

/* ========================================
   CONTAINER STYLES
   ======================================== */

QFrame {
    background-color: #252526;
    border: 1px solid #3e3e42;
    border-radius: 6px;
    padding: 0px;
}

QFrame[frameShape="0"] {
    border: none;
    background-color: transparent;
}

/* ========================================
   TYPOGRAPHY SYSTEM
   ======================================== */

QLabel {
    color: #cccccc;
    font-size: 13px;
    font-family: 'Segoe UI', 'SF Pro Display', -apple-system, BlinkMacSystemFont, Arial, sans-serif;
    font-weight: 400;
    background-color: transparent;
}

QLabel.heading {
    color: #ffffff;
    font-size: 16px;
    font-weight: 600;
    font-family: 'Segoe UI', 'SF Pro Display', -apple-system, BlinkMacSystemFont, Arial, sans-serif;
    letter-spacing: -0.2px;
}

QLabel.subheading {
    color: #cccccc;
    font-size: 14px;
    font-weight: 500;
    font-family: 'Segoe UI', 'SF Pro Display', -apple-system, BlinkMacSystemFont, Arial, sans-serif;
}

QLabel.small {
    font-size: 11px;
    color: #969696;
    font-weight: 500;
    text-transform: uppercase;
    letter-spacing: 0.5px;
}

/* Status labels with special colors */
QLabel#labelSpeed {
    color: #0078d4;
    font-weight: 600;
    font-family: 'Segoe UI', 'SF Pro Display', -apple-system, BlinkMacSystemFont, Arial, sans-serif;
}

QLabel#labelTime {
    color: #ff8c00;
    font-weight: 600;
    font-family: 'Segoe UI', 'SF Pro Display', -apple-system, BlinkMacSystemFont, Arial, sans-serif;
}

/* ========================================
   INPUT CONTROLS
   ======================================== */

QLineEdit, QPlainTextEdit, QTextEdit {
    border: 1px solid #3e3e42;
    border-radius: 4px;
    padding: 8px 12px;
    background-color: #1e1e1e;
    color: #cccccc;
    font-size: 13px;
    font-family: 'Consolas', 'Courier New', monospace;
    selection-background-color: #0078d4;
    selection-color: #ffffff;
}

QLineEdit:focus, QPlainTextEdit:focus, QTextEdit:focus {
    border: 2px solid #0078d4;
    background-color: #1e1e1e;
    outline: none;
    padding: 7px 11px; /* Compensate for 2px border */
}

QLineEdit:disabled, QPlainTextEdit:disabled, QTextEdit:disabled {
    background-color: #2d2d30;
    color: #6a6a6a;
    border-color: #3e3e42;
}

QLineEdit::placeholder, QPlainTextEdit::placeholder {
    color: #6a6a6a;
    font-style: italic;
}

QLineEdit:hover, QPlainTextEdit:hover, QTextEdit:hover {
    border-color: #4e4e52;
}

QLineEdit:focus:hover, QPlainTextEdit:focus:hover, QTextEdit:focus:hover {
    border-color: #0078d4;
}

/* ========================================
   TABLE VIEW STYLING
   ======================================== */

QTableView {
    border: 1px solid #3e3e42;
    border-radius: 6px;
    background-color: #252526;
    alternate-background-color: #2d2d30;
    gridline-color: #3e3e42;
    selection-background-color: #0078d4;
    selection-color: #ffffff;
    font-size: 13px;
    font-family: 'Segoe UI', 'SF Pro Display', -apple-system, BlinkMacSystemFont, Arial, sans-serif;
    outline: none;
}

QTableView::item {
    padding: 8px 12px;
    border-bottom: 1px solid #3e3e42;
    color: #cccccc;
    min-height: 28px;
    background-color: #252526;
}

QTableView::item:selected {
    background-color: #0078d4;
    color: #ffffff;
}

/* Note: Qt5 doesn't support complex pseudo-classes like :!selected and :alternate:hover:!selected */

/* ========================================
   TABLE HEADER STYLING
   ======================================== */

QHeaderView::section {
    background-color: #2d2d30;
    color: #cccccc;
    padding: 10px 12px;
    border: none;
    border-right: 1px solid #3e3e42;
    border-bottom: 2px solid #0078d4;
    font-weight: 600;
    font-size: 12px;
    font-family: 'Segoe UI', 'SF Pro Display', -apple-system, BlinkMacSystemFont, Arial, sans-serif;
    letter-spacing: 0.2px;
    text-transform: uppercase;
}

QHeaderView::section:first {
    border-top-left-radius: 6px;
}

QHeaderView::section:last {
    border-top-right-radius: 6px;
    border-right: none;
}

QHeaderView::section:hover {
    background-color: #333337;
    color: #ffffff;
}

QHeaderView::section:pressed {
    background-color: #0078d4;
    color: #ffffff;
}

/* ========================================
   BUTTON STYLING SYSTEM
   ======================================== */

QPushButton {
    background-color: #0078d4;
    color: #ffffff;
    border: none;
    border-radius: 6px;
    padding: 8px 16px;
    font-size: 13px;
    font-weight: 500;
    font-family: 'Segoe UI', 'SF Pro Display', -apple-system, BlinkMacSystemFont, Arial, sans-serif;
    min-width: 80px;
    min-height: 32px;
    letter-spacing: 0.1px;
    text-align: center;
}

QPushButton:hover {
    background-color: #106ebe;
}

QPushButton:pressed {
    background-color: #005a9e;
}

QPushButton:disabled {
    background-color: #3e3e42;
    color: #6a6a6a;
}

QPushButton:focus {
    border: 2px solid #0078d4;
    padding: 6px 14px;
}

/* Primary Action Buttons */
QPushButton#btnStart {
    background-color: #107c10;
    font-weight: 600;
}

QPushButton#btnStart:hover {
    background-color: #128a0f;
}

QPushButton#btnStart:pressed {
    background-color: #0d5e0d;
}

QPushButton#btnStart:focus {
    border: 2px solid #107c10;
}

/* Danger Action Buttons */
QPushButton#btnCancel {
    background-color: #e81123;
    font-weight: 600;
}

QPushButton#btnCancel:hover {
    background-color: #f1707a;
}

QPushButton#btnCancel:pressed {
    background-color: #c50f1f;
}

QPushButton#btnCancel:focus {
    border: 2px solid #e81123;
}

/* Warning Action Buttons */
QPushButton#btnDelete {
    background-color: #ff8c00;
    font-weight: 500;
}

QPushButton#btnDelete:hover {
    background-color: #ffa500;
}

QPushButton#btnDelete:pressed {
    background-color: #e67300;
}

QPushButton#btnDelete:focus {
    border: 2px solid #ff8c00;
}

/* Primary Add Button */
QPushButton#btnAddTasks {
    background-color: #107c10;
    font-weight: 600;
}

QPushButton#btnAddTasks:hover {
    background-color: #128a0f;
}

QPushButton#btnAddTasks:pressed {
    background-color: #0d5e0d;
}

QPushButton#btnAddTasks:focus {
    border: 2px solid #107c10;
}

/* ========================================
   MENU BAR AND MENU STYLING
   ======================================== */

QMenuBar {
    background-color: #2d2d30;
    color: #cccccc;
    font-family: 'Segoe UI', 'SF Pro Display', -apple-system, BlinkMacSystemFont, Arial, sans-serif;
    border-bottom: 1px solid #3e3e42;
}

QMenuBar::item {
    background-color: transparent;
    padding: 8px 16px;
    border-radius: 4px;
    font-weight: 500;
}

QMenuBar::item:selected {
    background-color: #0078d4;
    color: #ffffff;
}

QMenuBar::item:pressed {
    background-color: #005a9e;
    color: #ffffff;
}

QMenu {
    background-color: #2d2d30;
    border: 1px solid #4e4e52;
    border-radius: 6px;
    color: #cccccc;
    font-family: 'Segoe UI', 'SF Pro Display', -apple-system, BlinkMacSystemFont, Arial, sans-serif;
    padding: 4px 0;
}

QMenu::item {
    padding: 8px 20px;
    color: #cccccc;
    border-radius: 3px;
    margin: 1px 8px;
    min-height: 20px;
}

QMenu::item:selected {
    background-color: #0078d4;
    color: #ffffff;
}

QMenu::item:disabled {
    color: #6a6a6a;
}

QMenu::separator {
    height: 1px;
    background-color: #3e3e42;
    margin: 4px 12px;
}

/* ========================================
   STATUS BAR STYLING
   ======================================== */

QStatusBar {
    background-color: #2d2d30;
    color: #cccccc;
    font-size: 13px;
    font-family: 'Segoe UI', 'SF Pro Display', -apple-system, BlinkMacSystemFont, Arial, sans-serif;
    border-top: 1px solid #3e3e42;
}

QStatusBar::item {
    border: none;
    padding: 4px 8px;
}

/* ========================================
   PROGRESS BAR STYLING
   ======================================== */

QProgressBar {
    border: 1px solid #3e3e42;
    border-radius: 6px;
    text-align: center;
    font-weight: 600;
    background-color: #1e1e1e;
    color: #cccccc;
    font-family: 'Segoe UI', 'SF Pro Display', -apple-system, BlinkMacSystemFont, Arial, sans-serif;
    font-size: 12px;
    padding: 1px;
}

QProgressBar::chunk {
    background-color: #0078d4;
    border-radius: 5px;
}

/* ========================================
   MODERN SCROLLBAR STYLING
   ======================================== */

QScrollBar:vertical {
    border: none;
    background-color: #2d2d30;
    width: 12px;
    margin: 0;
    border-radius: 6px;
}

QScrollBar::handle:vertical {
    background-color: #4e4e52;
    border-radius: 6px;
    min-height: 24px;
    margin: 2px;
    border: none;
}

QScrollBar::handle:vertical:hover {
    background-color: #5e5e5e;
}

QScrollBar::handle:vertical:pressed {
    background-color: #0078d4;
}

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    height: 0px;
}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    background: none;
}

QScrollBar:horizontal {
    border: none;
    background-color: #2d2d30;
    height: 12px;
    margin: 0;
    border-radius: 6px;
}

QScrollBar::handle:horizontal {
    background-color: #4e4e52;
    border-radius: 6px;
    min-width: 24px;
    margin: 2px;
    border: none;
}

QScrollBar::handle:horizontal:hover {
    background-color: #5e5e5e;
}

QScrollBar::handle:horizontal:pressed {
    background-color: #0078d4;
}

QScrollBar::add-line:horizontal, QScrollBar::sub-line:horizontal {
    width: 0px;
}

QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
    background: none;
}

/* ========================================
   MODERN DIALOG AND MESSAGE BOX STYLING
   ======================================== */

QMessageBox {
    background-color: #2d2d30;
    color: #cccccc;
    border: 1px solid #4e4e52;
    border-radius: 8px;
    font-family: 'Segoe UI', 'SF Pro Display', -apple-system, BlinkMacSystemFont, Arial, sans-serif;
}

QMessageBox QPushButton {
    background-color: #0078d4;
    color: #ffffff;
    border: none;
    border-radius: 6px;
    padding: 8px 16px;
    font-size: 13px;
    font-weight: 500;
    font-family: 'Segoe UI', 'SF Pro Display', -apple-system, BlinkMacSystemFont, Arial, sans-serif;
    min-width: 80px;
    min-height: 32px;
    letter-spacing: 0.1px;
}

QMessageBox QPushButton:hover {
    background-color: #106ebe;
}

QMessageBox QPushButton:pressed {
    background-color: #005a9e;
}

QMessageBox QLabel {
    color: #cccccc;
    font-size: 13px;
    font-family: 'Segoe UI', 'SF Pro Display', -apple-system, BlinkMacSystemFont, Arial, sans-serif;
    background-color: transparent;
}

QMessageBox QLabel#qt_msgbox_label {
    color: #cccccc;
    font-size: 13px;
    font-family: 'Segoe UI', 'SF Pro Display', -apple-system, BlinkMacSystemFont, Arial, sans-serif;
    background-color: transparent;
}

QMessageBox QLabel#qt_msgboxex_icon_label {
    background-color: transparent;
}

QTextEdit#qt_msgbox_textedit {
    background-color: #1e1e1e;
    color: #cccccc;
    border: 1px solid #3e3e42;
    border-radius: 6px;
    font-size: 13px;
    font-family: 'Consolas', 'Courier New', monospace;
    selection-background-color: #0078d4;
    selection-color: #ffffff;
}

/* ========================================
   SPECIAL COMPONENTS
   ======================================== */

/* Line separator styling */
Line {
    background-color: #3e3e42;
}

/* Status frame styling */
QFrame#statusFrame {
    background-color: #2d2d30;
    border: 1px solid #3e3e42;
    border-radius: 6px;
}

/* ========================================
   FINAL VISUAL ENHANCEMENTS
   ======================================== */

/* Focus indicators for all interactive elements */
QLineEdit:focus,
QPlainTextEdit:focus,
QTextEdit:focus,
QTableView:focus,
QTableView::item:selected {
    outline: 2px solid #0078d4;
    outline-offset: 1px;
}

/* Enhanced disabled state styling */
*:disabled {
    color: #6a6a6a;
    opacity: 0.7;
